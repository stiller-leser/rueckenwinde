{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rueckenwinde/tagebuch",
  "title": "Tagebuch",
  "category": "widgets",
  "icon": "book",
  "description": "Zweispaltiger Tagebuch-Teaser mit Text und Bild",
  "keywords": ["tagebuch", "teaser", "gedanken"],
  "version": "1.0.0",
  "textdomain": "rueckenwinde",
  "supports": {
    "html": false,
    "anchor": true,
    "align": false
  },
  "attributes": {
    "heading": {
      "type": "string",
      "default": "UNSERE GEDANKEN"
    },
    "body": {
      "type": "string",
      "default": ""
    },
    "linkLabel": {
      "type": "string",
      "default": ">> ZUM TAGEBUCH"
    },
    "linkUrl": {
      "type": "string",
      "default": ""
    },
    "imageSrc": {
      "type": "string",
      "default": ""
    },
    "imageAlt": {
      "type": "string",
      "default": "Tagebuch Bild"
    }
  },
  "editorScript": "file:./editor.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css",
  "render": "file:./index.php"
}
