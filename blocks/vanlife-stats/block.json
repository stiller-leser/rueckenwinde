{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rueckenwinde/vanlife-stats",
  "title": "Vanlife Stats",
  "category": "widgets",
  "icon": "chart-bar",
  "description": "Statistiken für Vanlife-Reise (Distanz, Tage, Vanlife-Faktor, Budget)",
  "keywords": ["vanlife", "stats", "reise", "info"],
  "version": "1.0.0",
  "textdomain": "rueckenwinde",
  "supports": {
    "html": false,
    "anchor": true,
    "align": false
  },
  "attributes": {
    "distance": {
      "type": "string",
      "default": "2065 KM"
    },
    "days": {
      "type": "string",
      "default": "34 Tage"
    },
    "difficulty": {
      "type": "string",
      "default": "Easy"
    },
    "budget": {
      "type": "string",
      "default": "€€€"
    }
  },
  "editorScript": "file:./editor.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css",
  "render": "file:./index.php"
}
